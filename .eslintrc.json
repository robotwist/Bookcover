{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "max-len": ["error", { "code": 100 }],
    "no-underscore-dangle": "off"
  }
} 